    </xs:sequence>
  </xs:complexType>
  <xs:element name="Role" nillable="true" type="tns:Role" />
  <xs:complexType name="OperationResult">
    <xs:sequence>
      <xs:element minOccurs="0" name="Errors" nillable="true" type="tns:ArrayOfOperationError" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="OperationResult" nillable="true" type="tns:OperationResult" />
  <xs:complexType name="ArrayOfOperationError">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="OperationError" type="tns:OperationError" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfOperationError" nillable="true" type="tns:ArrayOfOperationError" />
  <xs:simpleType name="OperationError">
    <xs:restriction base="xs:string">
      <xs:enumeration value="LoginErr" />
      <xs:enumeration value="FullNameErr" />
      <xs:enumeration value="RoleErr" />
      <xs:enumeration value="PassErr" />
      <xs:enumeration value="UserExistsErr" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="OperationError" nillable="true" type="tns:OperationError" />
</xs:schema>